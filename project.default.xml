          <MavenGeneralSettings>
            <option name="mavenHome" value="C:/maven" />
            <option name="printErrorStackTraces" value="true" />
            <option name="usePluginRegistry" value="true" />
            <option name="userSettingsFile" value="C:\maven\conf\settings.xml" />
          </MavenGeneralSettings>
        </option>
        <option name="importingSettings">
          <MavenImportingSettings>
            <option name="vmOptionsForImporter" value="-Xmx768m" />
